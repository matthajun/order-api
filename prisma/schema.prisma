
generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./orders.db"
}

model Order {
  id        Int    @id @default(autoincrement())
  userId    Int
  productId Int
  quantity  Int
  user      User   @relation(fields: [userId], references: [id])
}

model User {
  id     Int     @id
  name   String
  orders Order[]
}
